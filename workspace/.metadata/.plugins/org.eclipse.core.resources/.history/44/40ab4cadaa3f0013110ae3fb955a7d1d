package TP5;

public static void main(String[] argv) {
    System.out.println("Entrez un nombre ");
    Integer nb = new Scanner(System.in).nextInt();
    try {
        System.out.println("fact(" + nb + ") : " + Factoriel.calc(nb));
    } catch (Exception ex) {
        System.out.println("Le nombre est trop grand !");
    }
}
}