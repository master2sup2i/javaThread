package ProdConsoExo6;

public class Produc extends Thread {
    private Buffer buf;
    private String nom;
    
    public Produc(Buffer b, String nom) {
        this.buf = b; 
        this.nom = nom;
    }
    
    public void run() {
    	for (int i = 0; i < 100; i++) {
    	       buf.put(i);

    	}
    	}
}

